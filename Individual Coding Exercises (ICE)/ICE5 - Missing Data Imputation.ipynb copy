    "# Set the missingness rate at 30%\n",
    "missing = 0.3\n",
    "\n",
    "# Only introduce missing rate to the `sepal length (cm)`.\n",
    "iris_MCAR.loc[iris_MCAR.sample(frac=missing).index, 'sepal length (cm)'] = np.nan\n",
    "\n",
    "iris_MCAR"
   ]
