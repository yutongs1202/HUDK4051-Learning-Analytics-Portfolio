   "outputs": [],
   "source": [
    "# Alternatively, iterate through all columns and introduce 20% missing to all variables.\n",
    "for col in iris_MCAR.columns:\n",
    "    iris_MCAR.loc[iris_MCAR.sample(frac=missing).index, col] = np.nan"
   ]
  },
