    "# Set the missingness rate at 30%\n",
    "missing = 0.3\n",
    "\n",
    "# Only introduce missing rate to the `sepal length (cm)`.\n",
    "iris_MAR.loc[iris_MAR.sample(frac=missing, weights = 'sepal width (cm)').index, 'sepal length (cm)'] = np.nan\n",
    "\n",
    "iris_MAR"
   ]
